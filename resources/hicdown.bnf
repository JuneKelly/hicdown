Document = (nl | (dnl)*) Block (dnl Block)* (nl | (dnl)*)
Block = TextContent

spc = #'\s'
dnl = #'\n(\n)+'
nl = !dnl #'\n'

TextContent = (TextRun | Segment)+

TextRun = (textchar+ | escape | nl)+

textchar = #'[^\n\[\]]'

escape = '\\[' | '\\]'

Segment = '[' tag spc+ TextContent+ ']'
tag = #':[^\s]+'
